<!DOCTYPE html>
<html>
  <head>
    <title>deepx</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= javascript_include_tag 'application'%>
    <%= stylesheet_link_tag  'application', media: 'all'%>
  </head>
  <body>
    <%= render 'home/header' %>
    <div class="container">
      <% if notice.present? %>
        <%= render 'layouts/notice' %>
      <%end%>
      <% if alert.present? %>
        <%= render 'layouts/alert' %>
      <%end%>
      <%= yield %>
    </div>
  </body>
</html>
